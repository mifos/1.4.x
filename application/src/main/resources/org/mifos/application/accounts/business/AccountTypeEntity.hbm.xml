<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

	<class name="org.mifos.application.accounts.business.AccountTypeEntity" table="ACCOUNT_TYPE">
		<id name="accountTypeId" column="ACCOUNT_TYPE_ID" type="short">
			<generator class="native" />
		</id>

		<property name="description" column="DESCRIPTION" type="string" />
		<property name="lookUpId" column="LOOKUP_ID" type="integer" not-null="true" />


	</class>

</hibernate-mapping>
