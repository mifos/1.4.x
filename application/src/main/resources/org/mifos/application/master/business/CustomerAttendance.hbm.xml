<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.master.business.CustomerAttendanceType" 
    	table="CUSTOMER_ATTENDANCE_TYPES">
        <id name="attendanceId" column="ATTENDANCE_ID" type="short">
            <generator class="native"/>
        </id>
        <property name="lookUpId" column="ATTENDANCE_LOOKUP_ID" type="integer" />
        <property name="desciption" column="DESCRIPTION" />
  	</class>
</hibernate-mapping>
        