<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping>

    <class name="org.mifos.application.master.business.FundCodeEntity" table="FUND_CODE">
        <id name="fundCodeId" column="FUNDCODE_ID" access="field">
            <generator class="native"/>
        </id>
        <property name="fundCodeValue" column="FUNDCODE_VALUE"/>
    </class>
</hibernate-mapping>