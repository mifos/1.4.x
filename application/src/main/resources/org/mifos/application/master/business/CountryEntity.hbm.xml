<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping > 
  <class name="org.mifos.application.master.business.CountryEntity" table="COUNTRY" >
    <id column="COUNTRY_ID" name="countryId" type="java.lang.Short" > 
		<generator class="native"></generator>
    </id>
	<property column="COUNTRY_NAME"  name="countryName"  type="java.lang.String"/>
	<property column="COUNTRY_SHORT_NAME"  name="countryShortName"  type="java.lang.String"/>
  </class> 
</hibernate-mapping>  