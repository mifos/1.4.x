	<queryString><![CDATA[select * from account a
join loan_account la on a.account_id = la.account_id
join loan_summary ls on a.account_id = ls.account_id
join customer c on a.customer_id = c.customer_id
left outer join loan_arrears_aging laa on a.account_id = laa.account_id
join loan_perf_history lph on a.account_id = lph.account_id
left outer join (select lsc.account_id, sum(lsc.principal_paid - lsc.principal) as principal_prepaid
  from loan_schedule lsc
  where lsc.payment_status = 1
  group by lsc.account_id
  having sum(lsc.principal_paid - lsc.principal) > 0) as ppp on a.account_id = ppp.account_id
left outer join (select lsc.account_id, sum(lsc.interest_paid - lsc.interest) as interest_prepaid
  from loan_schedule lsc
  where lsc.payment_status = 1
  group by lsc.account_id
  having sum(lsc.interest_paid - lsc.interest) > 0) as ipp on a.account_id = ipp.account_id
left outer join (select c.customer_id, sum(savings_balance) mbs from savings_account sa, account a, customer c
  where sa.account_id = a.account_id and a.customer_id = c.customer_id and sa.savings_type_id=1 
  group by c.customer_id) mbs on c.customer_id = mbs.customer_id
left outer join (select c.customer_id, sum(savings_balance) vbs from savings_account sa, account a, customer c
  where sa.account_id = a.account_id and a.customer_id = c.customer_id and sa.savings_type_id=2 
  group by c.customer_id) vbs on c.customer_id = vbs.customer_id
where a.personnel_id = $P{staffid}]]></queryString>

