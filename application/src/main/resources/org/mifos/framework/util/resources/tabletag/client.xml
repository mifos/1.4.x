			<parameter name="branchid" value="branch" valuetype="method"/>
		</parameters>
	</column>
	<column label="/" labeltype="string" boldlabel="true" type="link" action="/center">
		<displayname>
			<fragment-name name="center" type="method"/>
		</displayname>
